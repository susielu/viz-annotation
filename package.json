{
  "name": "viz-annotation",
  "version": "0.0.6",
  "description": "Shared drawing logic for handling annotations for data visualization.",
  "keywords": [
    "annotation",
    "data visualization"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-transform-es2015-spread": "6.22.0",
    "babel-preset-es2015": "6.18.0",
    "eslint": "3.8.1",
    "npm-watch": "0.1.6",
    "webpack": "3.8.1"
  },
  "dependencies": {
    "d3-shape": "~1"
  },
  "scripts": {
    "start": "npm run watch",
    "build": "babel src -d lib",
    "watch": "npm-watch",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Susie Lu",
    "url": "http://www.susielu.com"
  },
  "watch": {
    "build": "src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/susielu/viz-annotation.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/susielu/viz-annotation/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-es2015-spread"
    ]
  }
}
